
package ch.hearc.meteo.imp.afficheur.real.moo;

import org.jfree.data.time.TimeSeries;



public class StationDatas
	{

	/*------------------------------------------------------------------*\
	|*							Constructeurs							*|
	\*------------------------------------------------------------------*/

	public StationDatas(String name) {
		this(name, true);
	}

	public StationDatas(String name, boolean visible) {
		this.name = name;
		this.visible = visible;
	}

	/*------------------------------------------------------------------*\
	|*							Methodes Public							*|
	\*------------------------------------------------------------------*/

	/*------------------------------*\
	|*				Set				*|
	\*------------------------------*/

	/*------------------------------*\
	|*				Get				*|
	\*------------------------------*/

	/*------------------------------------------------------------------*\
	|*							Methodes Private						*|
	\*------------------------------------------------------------------*/

	/*------------------------------------------------------------------*\
	|*							Attributs Private						*|
	\*------------------------------------------------------------------*/
	private String name;
	private boolean visible;

	private TimeSeries seriesAltitude;
	private TimeSeries seriesPression;
	private TimeSeries seriesTemperature;

	}

